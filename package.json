{
  "name": "pbot",
  "version": "0.1.0",
  "description": "Powerb√§rs Bot",
  "author": "Victor Hallberg <victor@hallberg.cc>",
  "license": "ISC",
  "repository": "git+https://github.com/mogelbrod/pbot.git",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx . bot",
    "cli": "tsx .",
    "check": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint-staged": "lint-staged",
    "test": "vitest run",
    "test-watch": "vitest",
    "service": "tsx . --service bot",
    "service-install": "./install-system-service.sh",
    "service-logs": "journalctl --user -u pbot.service",
    "service-restart": "systemctl --user restart pbot",
    "service-start": "systemctl --user start pbot",
    "service-stop": "systemctl --user stop pbot"
  },
  "pre-commit": [
    "lint-staged",
    "test"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "prettier -w",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "airtable": "^0.12.2",
    "discord.js": "^14.18.0",
    "google-auth-library": "^10.5.0",
    "ms": "^2.1.3",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/ms": "^2.1.0",
    "@types/papaparse": "^5.5.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.0.0",
    "lint-staged": "^15.5.0",
    "pre-commit": "^1.0.10",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.16"
  }
}
